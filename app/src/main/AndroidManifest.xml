<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.designpatterndemo.chenxkang">

    <application
        android:allowBackup="true"
        android:icon="@drawable/design_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/design_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".principles.PrinciplesActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".patterns.PatternsActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>
